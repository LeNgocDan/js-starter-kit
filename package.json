{
  "name": "training-fe",
  "version": "1.0.0",
  "description": "Training FE",
  "scripts": {
    "sstart": "npm-run-all -p open:src backend",
    "open:src": "babel-node buildScripts/srcServer.js",
    "lt": "lt --port 3000 --subdomain jesse",
    "share": "npm-run-all --p open:src lt backend",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "pprebackend": "babel-node buildScripts/generateMockData",
    "start": "json-server --watch src/api/db.json --port 3001 --delay 500 --routes ./src/api/routes.json",
    "backend": "json-server --watch src/api/db.json --port 3001 --delay 500 --routes ./src/api/routes.json",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm run clean-dist",
    "build": "babel-node buildScripts/build.js",
    "ppostbuild": "babel-node buildScripts/distServer.js",
    "deploy": "surge ./dist"
  },
  "devDependencies": {
    "@babel/cli": "7.13.0",
    "@babel/core": "7.13.8",
    "@babel/node": "^7.13.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "7.13.8",
    "@babel/register": "7.13.8",
    "@faker-js/faker": "^6.0.0-alpha.7",
    "babel-loader": "8.2.2",
    "chai": "4.3.0",
    "chalk": "4.1.0",
    "cheerio": "1.0.0-rc.5",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "css-loader": "5.1.0",
    "eslint": "7.21.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-watch": "7.0.0",
    "express": "4.17.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "5.2.0",
    "jsdom": "^16.4.0",
    "json-schema-faker": "0.5.0-rcv.27",
    "json-server": "0.16.3",
    "localtunnel": "2.0.1",
    "mini-css-extract-plugin": "^1.3.9",
    "mocha": "8.3.0",
    "node-sass": "^7.0.1",
    "npm-run-all": "4.1.5",
    "numeral": "2.0.6",
    "open": "7.4.2",
    "rimraf": "3.0.2",
    "sass-loader": "^12.6.0",
    "style-loader": "2.0.0",
    "surge": "0.21.7",
    "webpack": "5.24.2",
    "webpack-dev-middleware": "4.1.0",
    "webpack-merge": "^5.8.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "bootstrap": "^5.2.3",
    "jquery": "^3.6.3",
    "tsparticles-engine": "^2.7.1"
  }
}
